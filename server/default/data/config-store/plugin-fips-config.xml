<?xml version="1.0" encoding="UTF-8"?>
<con:config xmlns:con="http://www.sourceid.org/2004/05/config">
    <!-- This map indicates the FIPS status of plugins. This is not a complete list of assessed plugins for a
         couple of reasons. Plugins may have been assessed since this file was last updated. In addition,
         plugins can indicate their FIPS status by setting the FipsStatus metadata value in the plugin's source
         code. If a value is defined in the source code and in this file, the value in the source code takes
         precedence. Keys in this map are fully-qualified class names of plugin implementations. Allowed values
         in the map are COMPLIANT or NOT_COMPLIANT. -->
    <con:map name="plugin-fips-status">
        <!-- COMPLIANT plugins -->
        <con:item name="com.pingidentity.pf.adapters.referenceid.IdpBackchannelReferenceAuthnAdapter">COMPLIANT</con:item>
        <con:item name="com.pingidentity.pf.adapters.referenceid.SpBackchannelReferenceAuthnAdapter">COMPLIANT</con:item>

        <!-- NOT_COMPLIANT plugins -->
        <con:item name="com.pingidentity.adapters.opentoken.IdpAuthnAdapter">NOT_COMPLIANT</con:item>
        <con:item name="com.pingidentity.adapters.opentoken.SpAuthnAdapter">NOT_COMPLIANT</con:item>
    </con:map>
</con:config>